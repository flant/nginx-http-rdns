ngx_addon_name=ngx_http_rdns_module

ngx_rdns_module_src="$ngx_addon_dir/ngx_http_rdns_module.c"

if test -n "$ngx_module_link"; then
	ngx_module_type=HTTP
	ngx_module_name=$ngx_addon_name
	ngx_module_srcs="$ngx_rdns_module_src"
	. auto/module
else
	HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
	NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_rdns_module_src"
fi
